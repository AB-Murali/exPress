{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "views": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "models": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}